{
  "name": "telegram-jung2-bot",
  "version": "5.0.0-alpha.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siutsin/telegram-jung2-bot.git"
  },
  "keywords": [
    "冗",
    "冗PowerBot",
    "冗員",
    "jung2bot",
    "jung2_bot",
    "telegram",
    "bot",
    "group",
    "chat",
    "stats"
  ],
  "author": "siutsin",
  "license": "MIT",
  "scripts": {
    "start": "node src/fastify.js",
    "nodemon": "nodemon src/fastify.js",
    "test": "npm run lint && nyc ava",
    "test_local": "npm run lint --fix && nyc ava --verbose",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov | codecov",
    "lint": "standard",
    "lint_fix": "standard --fix",
    "housekeeping": "./scripts/housekeeping.sh",
    "setup_dotenv": "./scripts/setup_dotenv.sh",
    "generate_seed": "node test/helper/generateDynamodbMessagesSeed.js && node test/helper/generateDynamodbChatIdSeed.js",
    "generate_stub_dynamodb": "node ./test/helper/generateStubDynamoDB.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "aws-sdk": "^2.983.0",
    "axios": "^0.21.4",
    "bottleneck": "^2.19.5",
    "dotenv": "^10.0.0",
    "fastify": "^3.20.1",
    "husky": "^7.0.1",
    "ip": "^1.1.5",
    "moment": "^2.29.1",
    "pino": "^7.0.2",
    "sqs-consumer": "^5.6.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "aws-sdk-mock": "^5.2.1",
    "codecov": "^3.8.3",
    "copy-dynamodb-table": "^2.2.1",
    "doctoc": "^2.0.1",
    "lodash.isstring": "^4.0.1",
    "nock": "^13.1.1",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "standard": "^16.0.3"
  }
