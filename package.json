{
  "name": "telegram-jung2-bot",
  "version": "4.4.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siutsin/telegram-jung2-bot.git"
  },
  "keywords": [
    "冗",
    "冗PowerBot",
    "冗員",
    "jung2bot",
    "jung2_bot",
    "telegram",
    "bot",
    "group",
    "chat",
    "stats"
  ],
  "author": "siutsin",
  "license": "MIT",
  "scripts": {
    "test": "npm run lint && nyc ava",
    "test_local": "npm run lint --fix && nyc ava --verbose",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov | codecov",
    "lint": "standard",
    "lint_fix": "standard --fix",
    "housekeeping": "./scripts/housekeeping.sh",
    "setup_dotenv": "./scripts/setup_dotenv.sh",
    "deploy": "./scripts/deploy.sh",
    "generate_seed": "node test/helper/generateDynamodbMessagesSeed.js && node test/helper/generateDynamodbChatIdSeed.js",
    "generate_stub_dynamodb": "node ./test/helper/generateStubDynamoDB.js",
    "offline": "kill $(lsof -t -i :8000); npm run generateSeed && sls offline start",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.14.6",
    "aws-sdk": "^2.941.0",
    "axios": "^0.21.1",
    "bottleneck": "^2.19.5",
    "ip": "^1.1.5",
    "moment": "^2.29.1",
    "pino": "^6.11.3",
    "pino-pretty": "^5.1.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "ava": "^2.4.0",
    "aws-sdk-mock": "^5.2.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-source-map-support": "^2.1.3",
    "codecov": "^3.8.2",
    "copy-dynamodb-table": "^2.2.1",
    "doctoc": "^2.0.1",
    "dotenv": "^10.0.0",
    "husky": "^7.0.1",
    "lodash.isstring": "^4.0.1",
    "nock": "^13.1.1",
    "nyc": "^15.1.0",
    "serverless": "^2.42.0",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-dynamodb-autoscaling": "^3.1.0",
    "serverless-plugin-lambda-dead-letter": "^1.2.1",
    "serverless-webpack": "^5.5.1",
    "standard": "^16.0.3",
    "webpack": "^5.43.0"
