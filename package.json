    "test": "npm run lint && nyc ava",
    "test-local": "npm run lint --fix && nyc ava --verbose",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov | codecov",
